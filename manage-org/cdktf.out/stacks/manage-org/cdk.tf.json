{
  "//": {
    "metadata": {
      "backend": "remote",
      "overrides": {
        "tfe_variable": [
          "lifecycle"
        ]
      },
      "stackName": "manage-org",
      "version": "0.9.4"
    },
    "outputs": {
    }
  },
  "data": {
    "tfe_team": {
      "team_owners": {
        "//": {
          "metadata": {
            "path": "manage-org/team_owners",
            "uniqueId": "team_owners"
          }
        },
        "name": "owners",
        "organization": "${var.organization}"
      }
    }
  },
  "provider": {
    "tfe": [
      {
        "hostname": "app.terraform.io"
      }
    ]
  },
  "resource": {
    "tfe_team_token": {
      "team_token": {
        "//": {
          "metadata": {
            "path": "manage-org/team_token",
            "uniqueId": "team_token"
          }
        },
        "team_id": "${data.tfe_team.team_owners.id}"
      }
    },
    "tfe_variable": {
      "provider_token": {
        "//": {
          "metadata": {
            "path": "manage-org/provider_token",
            "uniqueId": "provider_token"
          }
        },
        "category": "terraform",
        "key": "provider_token",
        "lifecycle": {
          "ignore_changes": "all"
        },
        "sensitive": true,
        "value": "${tfe_team_token.team_token.token}",
        "workspace_id": "${tfe_workspace.tfc_getting_started.id}"
      }
    },
    "tfe_workspace": {
      "tfc_getting_started": {
        "//": {
          "metadata": {
            "path": "manage-org/tfc_getting_started",
            "uniqueId": "tfc_getting_started"
          }
        },
        "auto_apply": true,
        "execution_mode": "remote",
        "lifecycle": {
          "ignore_changes": [
            "vcs_repo"
          ]
        },
        "name": "tfc-getting-started",
        "organization": "${var.organization}",
        "working_directory": "tfc-getting-started"
      }
    }
  },
  "terraform": {
    "backend": {
      "remote": {
        "hostname": "app.terraform.io",
        "organization": "${var.organization}",
        "workspaces": {
          "name": "manage-org"
        }
      }
    },
    "required_providers": {
      "tfe": {
        "source": "hashicorp/tfe",
        "version": "~> 0.26.1"
      }
    }
  },
  "variable": {
    "organization": {
      "description": "to be able to set Terraform Cloud Organization name via variable"
    }
  }
}